def is_prime(x):
    if x < 0:
        return False
    #0，1不是质数，2，3是质数
    elif x == 0 or x == 1:
        return False
    elif x == 2 or x == 3:
        return True
    else:
        for n in range(2, x-1):
            if x % n == 0 :
                return False
        else:
            return True
